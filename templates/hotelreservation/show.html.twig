{% extends 'baseBO.html.twig' %}

{% block title %}Hotelreservation{% endblock %}

{% block body %}
    <h1>Hotelreservation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ hotelreservation.id }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ hotelreservation.prix }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ hotelreservation.status }}</td>
            </tr>
            <tr>
                <th>Cratedat</th>
                <td>{{ hotelreservation.cratedat ? hotelreservation.cratedat|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Start</th>
                <td>{{ hotelreservation.start ? hotelreservation.start|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>End</th>
                <td>{{ hotelreservation.end ? hotelreservation.end|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('hotelreservation_index') }}">back to list</a>

    <a href="{{ path('hotelreservation_edit', {'id': hotelreservation.id}) }}">edit</a>

    {{ include('hotelreservation/_delete_form.html.twig') }}
{% endblock %}
